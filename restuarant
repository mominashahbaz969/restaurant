CREATE TABLE Restaurant (
    Restaurant_ID NUMBER PRIMARY KEY,
    Restaurant_Name VARCHAR2(100),
    City VARCHAR2(50),
    Cuisine_Type VARCHAR2(50),
    Rating NUMBER(2,1)
);

